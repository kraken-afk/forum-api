{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["xo"],
	"overrides": [
		{
			"extends": ["xo-typescript"],
			"files": ["*.ts", "*.tsx"]
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"@typescript-eslint/no-unsafe-assignment": 0,
		"@typescript-eslint/naming-convention": 0,
		"indent": 1,
		"@typescript-eslint/indent": 1
	}
}
