{
  "name": "forum-api",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles -i",
    "start": "node --experimental-modules target/release/main.js",
    "dev": "nodemon",
    "build": "node esbuild.mjs",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "prepare": "husky install",
    "generate": "drizzle-kit generate:pg --schema=./src/infrastructure/database/schema.ts --out=migrations",
    "migrate": "node utils/scripts/db-migrate.mjs",
    "migrate:test": "node utils/scripts/db-migrate-test.mjs"
  },
  "keywords": [],
  "author": "Romeo Noveanre",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@types/bcryptjs": "^2.4.4",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.2",
    "drizzle-kit": "^0.19.13",
    "esbuild": "^0.19.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "jsonwebtoken": "^9.0.2",
    "node-fetch-cjs": "^3.3.2",
    "ora": "6.3.0",
    "postgres": "^3.3.5",
    "zod": "^3.22.4"
  }
}